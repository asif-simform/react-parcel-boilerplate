{
  "name": "react-parcel-boilerplate",
  "version": "1.0.0",
  "description": "react-parcel-boilerplate",
  "private": false,
  "main": "public/index.html",
  "keywords": [
    "react",
    "parcel",
    "boilerplate"
  ],
  "author": {
    "name": "Asif Vora"
  },
  "license": "MIT",
  "scripts": {
