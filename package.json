    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.17",
    "mobx": "^5.15.1",
    "mobx-react": "^6.1.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-loading-skeleton": "^1.3.0",
    "react-router-dom": "^5.1.2",
    "react-toastify": "^5.4.1",
